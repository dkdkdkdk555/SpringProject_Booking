<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="com.acorn5.booking.users.dto.UsersDto"
			alias="usersDto"/>
		<typeAlias type="com.acorn5.booking.pay.dto.CartDto"
			alias="cartDto"/>
		<typeAlias type="com.acorn5.booking.review.dto.ReviewDto"
			alias="reviewDto"/>
		<typeAlias type="com.acorn5.booking.review.dto.ReviewCommentDto"
			alias="reviewCommentDto"/>
		<typeAlias type="com.acorn5.booking.order.dto.OrderDto"
			alias="orderDto"/>
	</typeAliases>
	<mappers>
		<mapper resource="com/acorn5/booking/mybatis/UsersMapper.xml"/>
		<mapper resource="com/acorn5/booking/mybatis/CartMapper.xml"/>
		<mapper resource="com/acorn5/booking/mybatis/ReviewMapper.xml"/>
		<mapper resource="com/acorn5/booking/mybatis/ReviewCommentMapper.xml"/>
